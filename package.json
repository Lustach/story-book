{
  "name": "storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@storybook/addon-storysource": "^7.0.6",
    "@storybook/addon-viewport": "^7.0.6",
    "@vueuse/core": "^9.13.0",
    "pinia": "2.0.32",
    "storybook-i18n": "^2.0.6",
    "vue": "^3.4.15",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-mdx-gfm": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.6",
    "@storybook/vue3-vite": "^7.0.6",
    "@testing-library/vue": "^8.0.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/ui": "^1.2.1",
    "@vue-macros/volar": "^0.12.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.0",
    "storybook": "^7.0.6",
    "typescript": "~4.8.4",
    "unplugin-vue-macros": "^2.3.7",
    "vite": "^4.1.4",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.2.0"
  }
}
